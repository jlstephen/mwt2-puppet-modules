[DEFAULT]
[cors]
[cors.subdomain]

[database]
connection = mysql+pymysql://glance:<%= @glance_db %>@<%= @controller_mgt_ip %>/glance

[glance_store]
stores = file,http
default_store = file
filesystem_store_datadir = /var/lib/glance/images/

[image_format]

[keystone_authtoken]
auth_uri = http://<%= @controller_mgt_ip %>:5000
auth_url = http://<%= @controller_mgt_ip %>:35357
memcached_servers = <%= @controller_mgt_ip %>:11211
auth_type = password
project_domain_name = default
user_domain_name = default
project_name = service
username = glance
password = <%= @glance_user %>

[matchmaker_redis]
[oslo_concurrency]
[oslo_messaging_amqp]
