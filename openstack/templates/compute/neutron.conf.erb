[DEFAULT]
transport_url = rabbit://openstack:<%= @rabbitmq %>@<%= @controller_mgt_ip %>
auth_strategy = keystone

[agent]
[cors]
[cors.subdomain]

[database]
connection = mysql+pymysql://neutron:<%= @neutron_db %>@<%= @controller_mgt_ip %>/neutron

[keystone_authtoken]
auth_uri = http://<%= @controller_mgt_ip %>:5000
auth_url = http://<%= @controller_mgt_ip %>:35357
memcached_servers = <%= @controller_mgt_ip %>:11211
auth_type = password
project_domain_name = default
user_domain_name = default
project_name = service
username = neutron
password = <%= @neutron_user %>

[matchmaker_redis]
[nova]

[oslo_concurrency]
lock_path = /var/lib/neutron/tmp

[oslo_messaging_amqp]
[oslo_messaging_kafka]
[oslo_messaging_notifications]
[oslo_messaging_rabbit]
[oslo_messaging_zmq]
[oslo_middleware]
[oslo_policy]
[qos]
[quotas]
[ssl]
